{
  "broker": {
    "name":"CB-BROKER",
    "host":${CB_HOST},
    "port":"${CB_PORT}",
    "clientId": "${GATEWAY_CLIENT_ID}",
    "security": {
      "caCert":"${CA_PATH}",
      "username": "${CB_USERNAME}",
      "password": "${CB_PASSWORD}"
    }
  },
  "mapping": [
    {
      "topicFilter": "${TOPIC_PREFIX}/devices/${DEVICE_PATTEN}/operational-data",
      "converter": {
        "type": "json",
        "deviceNameTopicExpression": "(?<=devices\/)(.*?)(?=\/operational-data)",
        "deviceTypeTopicExpression": "default",
        "timeout": 60000,
        "timeseries": [
          {
            "type": "json",
            "key": "CYCLEANALYTICS",
            "value": "${CYCLEANALYTICS}"
          }
        ]
      }
    }
  ,
    {
      "topicFilter": "${TOPIC_PREFIX}/devices/${DEVICE_PATTEN}/system-messages",
      "converter": {
        "type": "json",
        "deviceNameTopicExpression": "(?<=devices\/)(.*?)(?=\/system-messages)",
        "deviceTypeTopicExpression": "default",
        "timeout": 60000,
        "timeseries": [
          {
            "type": "json",
            "key": "MESSAGES",
            "value": "${MESSAGES}"
          }
        ]

      }
    },
    {
      "topicFilter": "${TOPIC_PREFIX}/devices/${DEVICE_PATTEN}/winload",
      "converter": {
        "type": "json",
        "deviceNameTopicExpression": "(?<=devices\/)(.*?)(?=\/winload)",
        "deviceTypeTopicExpression": "default",
        "timeout": 60000,
        "timeseries": [
          {
            "type": "json",
            "key": "WINLOAD",
            "value": "${WINLOAD}"
          }
        ]

      }
    },
    {
      "topicFilter": "${TOPIC_PREFIX}/devices/${DEVICE_PATTEN}/device-metadata",
      "converter": {
        "type": "json",
        "deviceNameTopicExpression": "(?<=devices\/)(.*?)(?=\/device-metadata)",
        "deviceTypeTopicExpression": "default",
        "timeout": 60000,
        "timeseries": [
          {
            "type": "json",
            "key": "ASSET",
            "value": "${ASSET}"
          }
        ]
      }
    }
  ]

}